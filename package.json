{
  "name": "rama-llama-95",
  "displayName": "Rama Llama 95",
  "description": "",
  "icon": "images/icon.jpg",
  "version": "0.0.1",
  "publisher": "click-here-to-begin",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rama-llama-95.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Rama Llama 95",
      "properties": {
        "rama-llama-95.url": {
          "type": "string",
          "default": "http://127.0.0.1:8080",
          "description": "OpenAI compatible API endpoint URL"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "url": "https://github.com/monai/rama-llama-95"
  }
}
